<!doctype html>
<html>
<head>
    <meta charset="UTF-8">
    <title>酒店管理</title>
    <link rel="stylesheet" href="../stylesheets/bootstrap.min.css">
    <style>
        .con_table{
            margin: 10px 50px;
            padding:50px;
            background-color: #eee;
            border: 1px solid #ddd;
            border-radius: 5px;
        }
    </style>
</head>
<body>
<div class="con_table">
    <form action="" class="form-inline">
        <div class="form-group"  style="margin-left:20px;">
            <label >电子预售券名称:</label>
            <input type="text" class="form-control">
            <label >电子预售券编号:</label>
            <input type="text" class="form-control">
            <label >年化奖励率(%):</label>
            <input type="text" class="form-control">
            <input type="button" class="btn btn-default" value="重置">
            <input type="button" class="btn btn-default" value="查询">
        </div>
        <div class="form-group  " style="margin-top:20px;margin-right:20px;margin-left: 20px;">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" data-toggle="modal" data-target="#addModal" value="新增">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" id="edit_hotel" value="编辑">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" id="delete" value="删除">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" id="delete" value="提交审核">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" id="delete" value="查看详情">
            <input type="button" class="btn btn-primary" style="margin-left: 10px;" id="delete" value="奖励计算器">
        </div>
    </form>
</div>
<div style="margin:0 50px;">
    <table class="table table-striped table-bordered table-hover">
        <thead>
        <tr>
            <th><input type="checkbox" name="sel" id="sel" alt="全选"/></th>
            <th>编号</th>
            <th>名称</th>
            <th>发行人</th>
            <th>年化奖励率(%)</th>
            <th>发行总额(元)</th>
            <th>起购金额(元)</th>
            <th>最大购买金额(元)</th>
            <th>开始发售日</th>
            <th>结束发售日</th>
            <th>审计状态</th>
        </tr>
        </thead>
        <tbody>
        <%
            var products=[
                {
                    product_id:'201612010506',
                    name:'彩虹计划-6个月',
                    issuer:'product-admin',
                    annual_rate:'7.80',
                    total_amount:'9000000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.01 11:00',
                    end_sell:'2017.03.30 24:00',
                    audit_status:'审核中'
                },
                {
                    product_id:'201612010510',
                    name:'彩虹计划-5个月',
                    issuer:'product-admin',
                    annual_rate:'7.60',
                    total_amount:'10000000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.01 11:00',
                    end_sell:'2017.03.30 24:00',
                    audit_status:'未提交'
                },
                {
                    product_id:'201612010515',
                    name:'彩虹计划-7个月',
                    issuer:'product-admin',
                    annual_rate:'9.60',
                    total_amount:'10000000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.02 11:00',
                    end_sell:'2017.03.10 24:00',
                    audit_status:'未通过'
                },
                {
                    product_id:'201512010506',
                    name:'理财计划-6个月',
                    issuer:'product-admin',
                    annual_rate:'7.80',
                    total_amount:'9000000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.01 11:00',
                    end_sell:'2017.03.30 24:00',
                    audit_status:'审核中'
                },
                {
                    product_id:'201512010315',
                    name:'理财计划-5个月',
                    issuer:'product-admin',
                    annual_rate:'7.60',
                    total_amount:'100000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.01 11:00',
                    end_sell:'2017.03.30 24:00',
                    audit_status:'未提交'
                },
                {
                    product_id:'201512010600',
                    name:'理财计划-7个月',
                    issuer:'product-admin',
                    annual_rate:'9.60',
                    total_amount:'10000000.00',
                    min_amount:'100',
                    max_amount:'10000',
                    start_sell:'2017.03.02 11:00',
                    end_sell:'2017.03.10 24:00',
                    audit_status:'未通过'
                }
            ]
        %>
        <% products.forEach(function(product){ %>
        <tr>
            <td><input type="checkbox" name="td_sel" class="td_sel"/></td>
            <td><%= product.product_id %></td>
            <td><%= product.name %></td>
            <td><%= product.issuer %></td>
            <td><%= product.annual_rate %></td>
            <td><%= product.total_amount %></td>
            <td><%= product.min_amount %></td>
            <td><%= product.max_amount %></td>
            <td><%= product.start_sell %></td>
            <td><%= product.end_sell %></td>
            <td><%= product.audit_status %></td>
        </tr>
        <% }) %>
        </tbody>
    </table>
    <div>
        <form action="" class="form-inline">
            共6条记录，第1/1页
            <div class="input-group">
                <input type="text" class="form-control" >
                <span class="input-group-btn">
                            <input type="button" class="btn btn-default" value="跳转">
                        </span>
            </div>
            <div class="pull-right">
                <a href="#">首页</a>
                <a href="#">上一页</a>
                <a href="#">下一页</a>
                <a href="#">尾页</a>
            </div>
        </form>
    </div>
</div>

<!-- 新增模态框(Modal) -->
<div class="modal fade" id="addModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">新增酒店信息</h4>
            </div>
            <form action="/collocation/hotel/add" method="post" class="form-horizontal" role="form">
                <div class="modal-body">
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店名称:</label>
                        <div class="col-sm-10">
                            <input type="text" name="hotel_name" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店地址:</label>
                        <div class="col-sm-10">
                            <input type="text" name="hotel_address" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店电话:</label>
                        <div class="col-sm-10">
                            <input type="text" name="phone" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">备注:</label>
                        <div class="col-sm-10">
                            <input type="text" name="note" class="form-control"/>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="submit" class="btn btn-primary" value="提交"/>
                    <input type="reset" class="btn btn-primary" value="重置"/>
                </div>
            </form>
        </div>
    </div>
</div>

<!-- 编辑模态框(Modal) -->
<div class="modal fade" id="editModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">编辑酒店信息</h4>
            </div>
            <form id="editForm" action="" method="post" class="form-horizontal" role="form">
                <div class="modal-body">
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店名称:</label>
                        <div class="col-sm-10">
                            <input type="text" id="hotel_name_e" name="hotel_name_e" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店地址:</label>
                        <div class="col-sm-10">
                            <input type="text" id="hotel_address_e" name="hotel_address_e" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">酒店电话:</label>
                        <div class="col-sm-10">
                            <input type="text" id="phone_e" name="phone_e" class="form-control"/>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="" class="col-sm-2 control-label">备注:</label>
                        <div class="col-sm-10">
                            <input type="text" id="note_e" name="note_e" class="form-control"/>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <input type="submit" class="btn btn-primary" value="更新"/>
                </div>
            </form>
        </div>
    </div>
</div>

<script type="text/javascript" src="../javascripts/jquery-1.12.0.min.js"></script>
<script type="text/javascript" src="../javascripts/bootstrap.min.js"></script>
<script type="text/javascript" src="../javascripts/hotel-common.js"></script>
</body>
</html>